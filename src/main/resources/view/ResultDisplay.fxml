<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="placeHolder" style="-fx-background-radius: 20;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <TextArea fx:id="resultDisplay" editable="false" style="-fx-border-color: #f3d5e2; -fx-border-width: 6;">
      <font>
         <Font name="Courier" size="13.0" />
      </font>
      <StackPane.margin>
         <Insets />
      </StackPane.margin></TextArea>
</StackPane>
