@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Interface ReadOnlySochedule <<Interface>>
Interface Model <<Interface>>
Interface ObservableList <<Interface>>
Class Sochedule
Class ReadOnlySochedule
Class Model
Class ModelManager
Class UserPrefs
Class ReadOnlyUserPrefs

Package Task {
Class Task
Class CompletionStatus
Class Priority
Class Deadline
Class PinnedStatus
Class UniqueTaskList
}

Package Event {
Class Event
Class StartDate
Class StartTime
Class EndDate
Class EndTime
Class UniqueEventList
}

Package Tag {
Class Tag
}

Package Category {
Class Category
}
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

Sochedule .up.|> ReadOnlySochedule

ModelManager .up.|> Model
Model .right.> ObservableList
ModelManager o--> "1" Sochedule
ModelManager o-left-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

Sochedule *--> "1" UniqueTaskList
UniqueTaskList o--> "*" Task
Task *--> Deadline
Task *--> CompletionStatus
Task *--> PinnedStatus
Task *--> Priority
Task *--> "*" Tag
Task *--> "*" Category

Sochedule *--> "1" UniqueEventList
UniqueEventList o--> "*" Event
Event *--> StartDate
Event *--> StartTime
Event *--> EndDate
Event *--> EndTime
Event *--> "*" Tag
Event *--> "*" Category

PinnedStatus -[hidden]right-> CompletionStatus
Deadline -[hidden]right-> Priority
CompletionStatus -[hidden]right-> Deadline

ModelManager ----->" ~* filtered list" Task
ModelManager ----->" ~* filtered list" Event
@enduml
